{
  "version": 2,
  "name": "NeeleyCompany12",
  "builds": [
    {
      "src": "test/index.py",
      "use": "@now/python@canary",
      "config": { "maxLambdaSize": "25mb" }
    },{
      "src": "test/now_build_staticfiles.sh",
      "use": "@now/static-build",
      "config": {
        "distDir": "static_root"
      }
    }
  ],
  "routes": [{"src": "/test/(.*)", "dest": "/"}, {
      "src": "/test/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}
