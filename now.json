{
  "version": 2,
  "name": "NeeleyCompany12",
  "builds": [
    {
        "src": "configuration/wsgi.py",
        "use": "@ardnt/now-python-wsgi",
      "config": { "maxLambdaSize": "25mb" }
    },{
      "src": "now_build_staticfiles.sh",
      "use": "@now/static-build",
      "config": {
        "distDir": "static_root"
      }
    }
  ],
  "routes": [{"src": "/(.*)", "dest": "/"}, {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}
