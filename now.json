{
  "version": 2,
  "name": "NeeleyCompany12",
  "builds": [
    {
      "src": "test/configurations/wsgi.py",
      "use": "@ardnt/now-python-wsgi",
      "config": { "maxLambdaSize": "25mb" }
    },{
      "src": "now_build_staticfiles.sh",
      "use": "@now/static-build",
      "config": {
        "distDir": "test/static_root"
      }
    }
  ],
  "routes": [{"src": "/(.*)", "dest": "test/configurations/wsgi.py"}, {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}
